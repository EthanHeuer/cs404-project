{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/all",
        "plugin:react-hooks/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks"
    ],
    "rules": {
      "comma-dangle": "off",
      "max-len": "off",
      "no-plusplus": "off",
      "react/jsx-indent": "off",
      "react/jsx-newline": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-curly-brace-presence": "off",
      "react/jsx-max-props-per-line": "off",
      "max-classes-per-file": "off",
      "jsx-quotes": "off"
    }
}
